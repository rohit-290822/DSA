class GfG
{
    Node deleteNode(Node head, int x)
    {
	Node temp=head;
	int c=1;

	while(temp.next!=null && temp.next.next!=null)
	{   
	  if(c==x)
        	{
        	    temp.data=temp.next.data;
        	    temp.next=temp.next.next;
        	    
        	}  
    
        temp=temp.next;
	       c+=1;
	}
	if(c==x)
	{ 
	     temp.data=temp.next.data;
        	    temp.next=temp.next.next;
	    
	}
	if(c==x-1)
	{
	    temp.next=null;
	}
	return head;
    }
}
