class Solution
{
    public int NthRoot(int n, int m)
    {
                  
                   
double low=1;
double high=m;
while(low<=high)
{   long mid=(int)(low+high)/2;
    int u=n;
    double ans=1;
    while(u>0)
    {
    ans=ans*mid;
    u-=1;
    
    }
    if(ans==m)
    {
        return (int)mid;
    }
    else if(ans>m)
    {
        high=mid-1;
      
    }
    else
    {
        low=mid+1;
      
    }

    }
    return -1;
    }
}
